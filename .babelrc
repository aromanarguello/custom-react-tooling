{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": ["last 2 versions", "not dead", "not < 2%", "not ie 11"]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["react-hot-loader/babel", "@babel/plugin-proposal-class-properties"] // allows us to run react classes b/c JS doesn't like the sintax
}
